#!/usr/bin/env zsh
 [[ "$(blueutil --is-connected 6c-b1-33-80-c0-89)" == '1' ]] && blueutil --disconnect 6c-b1-33-80-c0-89 && echo "Studio Wireless disconnected!" && exit 0

 [[ "$(blueutil --is-connected 6c-b1-33-80-c0-89)" == '0' ]] && blueutil --connect 6c-b1-33-80-c0-89 && echo "Studio Wireless connected!" && exit 0

