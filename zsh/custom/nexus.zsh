# Start Nexus Repository Manager Runtime
alias nexstart='/opt/nexus/nexus/bin/nexus start'

# Check Status Nexus Repository Manager Runtime
alias nexstatus='/opt/nexus/nexus/bin/nexus status'

# Stop Status Nexus Repository Manager Runtime
alias nexstop='/opt/nexus/nexus/bin/nexus stop'

# Private Repository
alias npxpd='npx install-peerdeps -r http://localhost:8081/repository/npm-group/'
